<?xml version="1.0" encoding="UTF-8"?>
<blackBoxStochModel xmlns="http://www.openda.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.openda.org http://schemas.openda.org/blackBoxStochModelConfig.xsd">

    <modelConfig>
        <file>./dflowfmModel.xml</file>
    </modelConfig>

    <vectorSpecification>
			<exchangeItems>
				<exchangeItem id="K1_amplitude" modelExchangeItemId="tide_west_ext_0002.K1_amplitude"/>
				<exchangeItem id="K1_phase" modelExchangeItemId="tide_west_ext_0002.K1_phase"/>
			</exchangeItems>
        
			<parameters>
					<regularisationConstant>
							<stdDev value=".05" transformation="identity"/>
							<vector id="K1_amplitude"/>
					</regularisationConstant>
					<regularisationConstant>
							<stdDev value="5.0" transformation="identity"/>
							<vector id="K1_phase"/>
					</regularisationConstant>
        </parameters>

				<predictor>
					<!--
					<vector id="Pilots_Station.waterlevel"/>
					<vector id="Dauphin_Island.waterlevel"/>
					-->
					<vector id="Calcasieu_Pass.waterlevel"/>
        </predictor>

    </vectorSpecification>

</blackBoxStochModel>
